#include <iostream>
 
using namespace std;
 
int main() {
 
    string message;
    while(cin>>message)
    {
        int number,index;
        cin>>number;
       
        for(int i=0;i<number;i++)
        {
            cin>>index;
            cout<<message[index-1];
        }
        cout<<endl;
    }
    return 0;
}
