#include <iostream>
 
using namespace std;
 
int main() {
 
    double N[12][12];
    char a;
    double sum = 0.0;
    int count=0;
    
    cin >> a;
    
    for(int i = 0; i < 12; i++)
    {
            for(int j = 0; j < 12; j++)
            {
                   cin >> N[i][j];
                   
            if(j > i  && (j+i) < 11)
            {
                 sum += N[i][j];
                 count++;
            }
            }
    }
    
    if(a == 'S')
         printf("%.1f\n", sum);
    else
         printf("%.1f\n", sum/count);
 
    return 0;
}
