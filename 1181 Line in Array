#include <iostream>
 
using namespace std;
 
int main() {
 
    double sum=0.0, M[12][12];
    char T[2];
    int c,x,y;
    cin>>c;
    cin>>T;
    
    for(x=0;x<=11;x++)
    {
        for(y=0; y<=11; y++)
        {
            
        cin>>M[x][y];
        if(x==c)
        
        sum+=M[x][y];
        
        }
    }
    
    if(T[0]=='S')
    printf("%.1lf\n",sum);
        
    else if(T[0]=='M')
    {
        sum=sum/12.0;
        printf("%.1lf\n",sum);
    }
 
    return 0;
}
