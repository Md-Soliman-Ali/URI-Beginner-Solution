#include <iostream>
 
using namespace std;
 
int main() 
{
    int M,m=0,n=0,k=0;
    
    while(1)
    {
        cin>>M;
        if(M==1)
        m++;
        if(M==2)
        n++;
        if(M==3)
        k++;
        if(M==4)
        break;
        
    }
    
    cout<<"MUITO OBRIGADO\n";
    cout<<"Alcool: "<<m<<endl;
    cout<<"Gasolina: "<<n<<endl;
    cout<<"Diesel: "<<k<<endl;
}
