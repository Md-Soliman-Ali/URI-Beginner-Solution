#include <iostream>
#include <stdio.h>
#include<math.h>
 
using namespace std;
 
int main() {
 
     float x1,y1,x2,y2,p1,p2,Distance;
     cin>>x1;
     cin>>y1;
     cin>>x2;
     cin>>y2;

    p1=x2-x1;
    p2=y2-y1;
    
    Distance=sqrt((p1*p1)+(p2*p2));
    
    printf("%.4f\n",Distance);
      
    return 0;
}

